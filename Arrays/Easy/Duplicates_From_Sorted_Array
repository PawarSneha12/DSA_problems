LeetCode 26

class Solution {
    public int removeDuplicates(int[] arr) {
        if(arr.length==0){return 0;}
        int i=0;
        for(int j=1;j<arr.length;j++){
            if(arr[j]!=arr[i]){
                i++;
                // //increment start pointer
                arr[i]=arr[j];
                //place the next unique value to start pointer, so all duplicate will bre removed
                }
        }
        return i+1;
        
    }
}
