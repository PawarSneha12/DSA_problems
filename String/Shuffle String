class Solution {
    public String restoreString(String s, int[] indices) {
        
         StringBuilder st1=new StringBuilder(s);
         char []ch=s.toCharArray();
        
        for(int i=0;i<indices.length;i++){
            int in=indices[i];
            st1.setCharAt(in,ch[i]);
        }
return st1.toString();
        
    }
}
