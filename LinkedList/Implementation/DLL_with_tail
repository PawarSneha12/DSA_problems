public void insertFirst(int val){
    Node node =new Node(val);
    node.prev=null;
    if(head==null){
        head=node;
        tail=node;
    }
    else{
    node.next=head;
    head.prev=node;
    head=node;
     }
    size++;
}

public void inserstLastTail(int val){
    Node node=new Node(val);
    node.next=null;
    if(tail==null){
        node.prev=null;
        head=node;
        tail=null;
}
else{
    node.prev=tail;
    tail.next=node;
    tail=node;

}
size++;
}
